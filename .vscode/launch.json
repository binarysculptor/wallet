    "version": "0.2.0",
    "configurations": [
    {
        "name": "Mac Debug",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/src/qt/liberty-qt",
        "args": ["-debug", "-printstakemodifier"],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": true,
        "MIMode": "lldb"
    },
        {
            "name": "Debug",
            "type": "gdb",
            "request": "launch",
            "target": "./src/qt/liberty-qt",
            "cwd": "${workspaceRoot}"
        }
    ]
}